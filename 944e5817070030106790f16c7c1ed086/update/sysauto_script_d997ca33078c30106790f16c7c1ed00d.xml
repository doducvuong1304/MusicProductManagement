        <script><![CDATA[var rightNow = new GlideDateTime();


var overdueMP = new GlideRecord('x_625175_music_pro_music_product');
overdueMP.addQuery('release_date', '<=', rightNow);
overdueMP.addQuery('status', '!=', 'completed');
overdueMP.addQuery('released', '!=', 'True');

overdueMP.query();

while (overdueMP.next()) {
    gs.info("Overdue Music Product: " + overdueMP.name);
    gs.eventQueue('x_625175_music_pro.overdueMusicProduct', overdueMP, overdueMP.name, gs.getUserName());
}]]></script>
